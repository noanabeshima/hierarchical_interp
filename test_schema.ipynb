    "import jsonschema\n",
    "import json\n",
    "\n",
    "with open('tree.schema.json', 'r') as f:\n",
    "    schema = json.load(f)\n",
    "\n",
    "\n",
    "def validate_tree(instance):\n",
    "    jsonschema.validate(instance, schema)\n",
    "\n",
    "with open('tree.json') as f:\n",
    "    tree_dict = json.load(f)\n",
    "\n",
    "validate_tree(tree_dict)"
   ]
